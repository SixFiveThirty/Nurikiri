<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.nurikiri.mapper.StoreAdminMapper">
        <select id="storeAdminList" resultType="StoreAdminVO">
            select sno,title,regDate, is_deleted
            from store
        </select>
        
        <select id="getListWithPaging" resultType="StoreAdminVO">
		select title from store
		
		order by sno desc
		limit #{offset}, #{amount}
	</select>
        
      
</mapper>
